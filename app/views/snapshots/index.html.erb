<p id="notice"><%= notice %></p>

<h1>Snapshots</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Alpa2 code</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Deaths</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @snapshots.each do |snapshot| %>
      <tr>
        <td><%= snapshot.location %></td>
        <td><%= snapshot.alpa2_code %></td>
        <td><%= snapshot.latitude %></td>
        <td><%= snapshot.longitude %></td>
        <td><%= snapshot.deaths %></td>
        <td><%= link_to 'Show', snapshot %></td>
        <td><%= link_to 'Edit', edit_snapshot_path(snapshot) %></td>
        <td><%= link_to 'Destroy', snapshot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Snapshot', new_snapshot_path %>
